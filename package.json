{
  "name": "xboilerplate",
  "version": "1.3.2",
  "description": "boilerplate for slicing project",
  "main": "index.js",
  "scripts": {
    "open:dev:hmr": "webpack-dev-server --config webpack.dev.hot.js --inline --hot --watch",
    "open:dev": "webpack --watch --config webpack.dev.js",
    "open:prod": "webpack --config webpack.prod.js",
    "test": "jest --watch --verbose",
    "flow": "flow",
    "lint": "esw src --color",
    "lint:watch": "npm run lint -- --watch",
    "stylelint": "stylelint **/*.scss",
    "prestart": "babel-node tools/nodeVersionCheck.js",
    "start": "npm-run-all --parallel open:dev lint:watch",
    "prebuild": "babel-node tools/nodeVersionCheck.js && npm run lint",
    "build": "npm run open:prod && babel-node tools/distServer.js",
    "analyze": "webpack --config webpack.analyze.js"
  },
  "repository": {
    "type": "git",
    "url": "http://rudy@stash.xtremax.com/scm/xslic/xboilerplate.git"
  },
  "author": "Rudy Prihantoro",
  "license": "ISC",
  "dependencies": {
    "@xtremax/embeded-iframe": "1.0.1",
    "bootstrap": "4.0.0-beta.2",
    "d3": "5.11.0",
    "font-awesome": "4.7.0",
    "jquery": "3.4.1",
    "popper.js": "1.12.5",
    "vanilla-lazyload": "8.15.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.4.0",
    "autoprefixer": "7.1.5",
    "babel-eslint": "8.2.4",
    "babel-jest": "25.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-flow": "6.23.0",
    "browser-sync": "2.26.7",
    "browser-sync-webpack-plugin": "1.2.0",
    "chalk": "^2.3.0",
    "clean-webpack-plugin": "0.1.17",
    "css-loader": "3.0.0",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.17.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "5.1.2",
    "expose-loader": "0.7.5",
    "extract-loader": "3.0.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "2.0.0",
    "flow-bin": "0.76.0",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "4.6.0",
    "jest": "24.8.0",
    "jest-jquery-matchers": "2.1.0",
    "node-sass": "4.9.3",
    "npm-run-all": "^4.1.2",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "2.0.8",
    "prettier-eslint": "8.2.1",
    "prettier-stylelint": "0.4.2",
    "pug": "2.0.4",
    "pug-html-loader": "1.1.5",
    "pug-lint": "2.5.0",
    "purify-css": "1.2.5",
    "purifycss-webpack": "0.7.0",
    "sass-loader": "6.0.6",
    "semver": "^5.4.1",
    "shelljs": "^0.7.8",
    "style-loader": "0.19.0",
    "stylelint": "10.1.0",
    "webpack": "4.19.0",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.14",
    "webpack-import-glob-loader": "1.6.3",
    "webpack-merge": "4.1.0",
    "workbox-webpack-plugin": "3.5.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
